<Window x:Class="LifeUIWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:LifeUIWPF"
        Title="MainWindow" Height="506.201" Width="495.288">
    <Grid x:Name="content" Loaded="content_Loaded">
        <Grid.RowDefinitions>
            <RowDefinition Height="32" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Grid.Column="0" Background="AliceBlue" Orientation="Horizontal">
            <Button Name="btnStart" Margin="0,0,10,0" Click="btnStart_Click">Start</Button>
            <Button Name="btnReset" Margin="0,0,10,0" Click="btnReset_Click">Reset</Button>
        </StackPanel>
    </Grid>

</Window>
